//----------------------------------------------------------------------------
//  The reagent enchantment definitions are used by the magical reagent objects.
//  Just place whatever enchantments you want within a block that you name,
//  and those enchantments will be attached to the reagent object that points to
//  that definitions in the [magic] block.
//----------------------------------------------------------------------------
//////////////////////////////////////////////////////////////////////////////

[reagent_definitions]
{
	[reagent_test]
	{
		object_types = weapon;		// Can be any of the pcontent base or specfic types.
		skill_types = fighter;		// fighter, ranger, mage, nmage, cmage
		[strength]
		{
			alteration			= alter_strength;
			value				= 1;
			min_value_ceiling	= 5;
			description			= "+<value> Strength";
			duration			= #infinite;
			is_permanent		= true;
			is_single_instance	= false;
			custom_effect		= "rgt_strength";	// must be unique to effect
			custom_effect_skrit = world\global\enchantments\weapon_enchant.skrit
									  ?weapon_ffx		= ffx_spell_str_bonus_sword
									  &projectile_ffx	= ffx_spell_str_bonus_arrow;
		}
	}

